[run]
source = app
omit =
    */migrations/*
    */tests/*
    */venv/*
    */.venv/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    if TYPE_CHECKING:

[html]
directory = coverage_html
